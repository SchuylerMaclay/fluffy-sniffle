<!DOCTYPE html>
<html>
<head>
  <title>RailsQwitter</title>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

  <div class="">
    <h1>Qwitter</h1>
  </div>

</head>
<body>
  <div class="">
    <% if flash[:error] %>
    <%= flash[:error] %>
    <% end %>
  </div>
  <div class="">
    <% if flash[:notice] %>
    <%= flash[:notice] %>
<% end %>
  </div>
<div class="">
    <% if session[:message] %>
    Session Message: <%= session[:message] %>
    <% end %>
</div>

  <ul>
    <li><%= link_to "user index", users_path %></li>
  </ul><%= yield %>

</body>
</html>
